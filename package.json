{
  "name": "transactionsappmobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn run android:local",
    "android:local": "cd android && ./gradlew assembleLocal && react-native run-android --mode=localDebug",
    "android:qa": "cd android && ./gradlew assembleQa && react-native run-android --mode=qaDebug",
    "android:prod": "cd android && ./gradlew assembleProd && react-native run-android --mode=prodRelease",
    "android:bundle": "cd android && ./gradlew bundleProdRelease",
    "ios": "react-native run-ios --simulator=\"iPhone 15\"",
    "ios:local": "react-native run-ios --simulator=\"iPhone 15\" --mode Debug",
    "ios:staging": "react-native run-ios --simulator=\"iPhone 15\" --mode Staging",
    "ios:release": "react-native run-ios --simulator=\"iPhone 15\" --mode Release",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.2.0",
    "axios": "^1.2.4",
    "chalk": "^4.1.2",
    "prop-types": "^15.7.2",
    "react": "18.2.0",
    "react-native": "^0.73.0",
    "react-native-currency-input": "^1.1.1",
    "react-native-date-picker": "4.2.1",
    "react-native-eject": "^0.2.0",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-reanimated": "^3.6.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.21.0",
    "react-native-vector-icons": "^10.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0-0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0-0",
    "@babel/plugin-transform-shorthand-properties": "^7.0.0-0",
    "@babel/plugin-transform-template-literals": "^7.0.0-0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.0",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.0",
    "@react-native/typescript-config": "0.73.1",
    "@types/babel__core": "^7.20.0",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.2.1",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
